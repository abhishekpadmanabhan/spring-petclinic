pipeline {
       agent {
           label 'master'
             }
           tools {
             maven 'myMaven'
             jdk 'myJava'
                }
            stages {
               stage ('Checkout the code')
             {
                steps {
                 git branch 'master', url: 'https://github.com/abhishekpadmanabhan/addressbook.git'
                 }
             }
                stage ('Code Compile') {
               steps {
                    sh """
                    mvn compile
                    """
                    }
                  }
        stage ('JUNIT Test') {
            steps{
                sh """
                mvn test
                """
            }
        }
                 stage ('Packaging') {
                    steps {
                        sh """
                        mvn package
                        """
                         }
                    }
   }
}
